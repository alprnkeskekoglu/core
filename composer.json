{
    "name": "dawnstar/dawnstar",
    "description": "Package for Dawnstar CMS",
    "license": "MIT",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "intervention/image": "^2.4@dev",
        "dawnstar/filemanager": "*"
    },
    "autoload": {
        "psr-4": {
            "Dawnstar\\": "src"
        },
        "files": [
            "src\\Http\\helpers.php"
        ]
    },
    "config": {
        "sort-packages": true,
        "classmap-authoritative": true,
        "apcu-autoloader": true,
        "optimize-autoloader": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Dawnstar\\DawnstarServiceProvider"
            ]
        }
    }
}
