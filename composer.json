{
    "name": "dawnstar/core",
    "description": "Package for Dawnstar CMS",
    "license": "MIT",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "dawnstar/media-manager": "dev-master",
        "dawnstar/module-builder": "dev-master",
        "dawnstar/region": "dev-master",
        "dawnstar/developer": "dev-master",
        "dawnstar/tracker": "^1.0",
        "spatie/laravel-permission": "^5.4",
        "yajra/laravel-datatables-oracle": "~9.0"
    },
    "autoload": {
        "psr-4": {
            "Dawnstar\\Core\\": "src"
        }
    },
    "extra": {
